{
  "name": "@fjell/providers",
  "description": "Providers for Fjell",
  "version": "4.4.0",
  "license": "Apache-2.0",
  "main": "./dist/src/index.js",
  "exports": {
    ".": {
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "dev": "concurrently \"tsc --watch\" \"tsc-alias -w\"",
    "build": "yarn run lint && tsc --build --verbose --force && tsc-alias -p tsconfig.json",
    "clean": "rimraf dist",
    "test": "yarn run lint && NODE_OPTIONS='--experimental-require-module' yarn run jest --coverage"
  },
  "dependencies": {
    "@fjell/cache": "^4.5.1",
    "@fjell/core": "^4.3.4",
    "@fjell/logging": "^4.3.8",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@tsconfig/recommended": "^1.0.8",
    "@types/babel__preset-env": "^7.10.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "tsc-alias": "^1.8.11",
    "typescript": "^5.8.2"
  },
  "packageManager": "yarn@4.6.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/providers.git"
  }
}
