{
  "name": "@fjell/providers",
  "description": "Providers for Fjell",
  "version": "4.5.88-dev.0",
  "license": "Apache-2.0",
  "keywords": [
    "react-providers",
    "fjell"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "dev": "node build.js --watch",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rm -rf dist",
    "test": "npm run lint && vitest run --coverage",
    "precommit": "npm run clean && npm run lint && npm run build && npm run test",
    "prepublishOnly": "npm run clean && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/cache": "^4.7.63",
    "@fjell/client-api": "^4.4.70",
    "@fjell/core": "^4.4.76",
    "@fjell/logging": "^4.4.66",
    "@fjell/types": "^4.4.6",
    "react-error-boundary": "^6.0.0"
  },
  "peerDependencies": {
    "react": "^19.1.2",
    "react-dom": "^19.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@fjell/common-config": "^1.1.36",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@tsconfig/recommended": "^1.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitest/coverage-v8": "4.0.2",
    "esbuild": "0.25.11",
    "eslint": "^9.38.0",
    "jsdom": "^27.0.1",
    "react": "^19.1.2",
    "react-dom": "^19.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/providers.git"
  }
}
